<p id="notice"><%= notice %></p>

<h1>Corr Histories</h1>

<table>
  <thead>
    <tr>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @corr_histories.each do |corr_history| %>
      <tr>
        <td><%= corr_history.team_id %></td>
        <td><%= link_to 'Show', corr_history %></td>
        <td><%= link_to 'Edit', edit_corr_history_path(corr_history) %></td>
        <td><%= link_to 'Destroy', corr_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Corr History', new_corr_history_path %>
