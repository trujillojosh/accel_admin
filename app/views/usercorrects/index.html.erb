<p id="notice"><%= notice %></p>

<h1>Usercorrects</h1>

<table>
  <thead>
    <tr>
      <th>P1</th>
      <th>P2</th>
      <th>P3</th>
      <th>P4</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usercorrects.each do |usercorrect| %>
      <tr>
        <td><%= usercorrect.p1 %></td>
        <td><%= usercorrect.p2 %></td>
        <td><%= usercorrect.p3 %></td>
        <td><%= usercorrect.p4 %></td>
        <td><%= link_to 'Show', usercorrect %></td>
        <td><%= link_to 'Edit', edit_usercorrect_path(usercorrect) %></td>
        <td><%= link_to 'Destroy', usercorrect, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Usercorrect', new_usercorrect_path %>
