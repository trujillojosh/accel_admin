
<%= simple_form_for(@attendance) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.input :title %>
    <%= f.input :date %>
    <%= f.input :desc %>
    <%= f.input :here %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
