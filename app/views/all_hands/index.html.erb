<p id="notice"><%= notice %></p>

<h1>All Hands</h1>

<table>
  <thead>
    <tr>
      <th>Team</th>
      <th>Date</th>
      <th>B kpi</th>
      <th>T kpi</th>
      <th>Help</th>
      <th>Blockers</th>
      <th>Other</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @all_hands.each do |all_hand| %>
      <tr>
        <td><%= all_hand.team.name %></td>
        <td><%= all_hand.date %></td>
        <td><%= all_hand.b_kpi %></td>
        <td><%= all_hand.t_kpi %></td>
        <td><%= all_hand.help %></td>
        <td><%= all_hand.blockers %></td>
        <td><%= all_hand.other %></td>
        <td><%= link_to 'Show', all_hand %></td>
        <td><%= link_to 'Edit', edit_all_hand_path(all_hand) %></td>
        <td><%= link_to 'Destroy', all_hand, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New All Hand', new_all_hand_path %>
